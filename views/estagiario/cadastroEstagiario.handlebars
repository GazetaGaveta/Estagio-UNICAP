<h3>Novo estagiário</h3>
<form action="/estagiarios/cadastro/novo" method="POST">
    <div>
        <label for="nome">Nome: </label>
            <input type="text" id="nome" name="nome" placeholder="nome" class="form-control-plaintex" value={{nome}}>
        <br>
        <label for="dataNascimento">Data de nascimento: </label>
            <input type="text" id="dataNascimento" name="dataNascimento" placeholder="dd/mm/AAAA" class="form-control-plaintex" value={{dataNascimento}}>
        <br>
        <label for="registroAcademico">Registro academico: </label>
            <input type="text" id="registroAcademico" name="registroAcademico" placeholder="registro academico" class="form-control-plaintex" value={{registroAcademico}}>
        <br>
        <label for="curso">Curso: </label>
            <input type="text" id="curso" name="curso" placeholder="curso" class="form-control-plaintex" value={{curso}}>
        <br>
        <label for="turno">Turno: </label>
            <input type="text" id="turno" name="turno" placeholder="turno" class="form-control-plaintex" value={{turno}}>
        <br>
        <label for="cedulaDeIdentidade">Cedula de identidade: </label>
            <input type="text" id="cedulaDeIdentidade" name="cedulaDeIdentidade" placeholder="cedula de identidade" class="form-control-plaintex" value={{cedulaDeIdentidade}}>
        <br>
        <label for="cpf">Cpf: </label>
            <input type="text" id="cpf" name="cpf" placeholder="cpf" class="form-control-plaintex" value={{cpf}}>
        <br>
        <label for="nacionalidade">Nacionalidade: </label>
            <input type="text" id="nacionalidade" name="nacionalidade" placeholder="nacionalidade" class="form-control-plaintex" value={{nacionalidade}}>
        <br>
        <label for="estadoCivil">Estado civil: </label>
            <input type="text" id="estadoCivil" name="estadoCivil" placeholder="estado civil" class="form-control-plaintex" value={{estadoCivil}}>
        <br>
        <label for="cidade">Cidade: </label>
            <input type="text" id="cidade" name="cidade" placeholder="cidade" class="form-control-plaintex" value={{cidade}}>
        <br>
        <label for="estado">Estado: </label>
            <input type="text" id="estado" name="estado" placeholder="estado" class="form-control-plaintex" value={{estado}}>
        <br>
        <label for="cep">Cep: </label>
            <input type="text" id="cep" name="cep" placeholder="cep" class="form-control-plaintex" value={{cep}}>
        <br>
        <label for="fone">Fone: </label>
            <input type="text" id="fone" name="fone" placeholder="telefone" class="form-control-plaintex" value={{fone}}>
        <br>
        <label for="email">Email: </label>
            <input type="text" id="email" name="email" placeholder="email" class="form-control-plaintex" value={{email}}>
        <br>
        <button>Cadastrar estagiário</button>
    </div>
</form>